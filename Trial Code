 * Class ContactList and Class Person.
 * @author micaelakowalczyk
 * Edited by BK.
 */
package contact_list;
import java.util.*;
public class QualitySoft {
	public static void main(String[] args) {

		String firstName,lastName,address,email,phone,notes;
		Scanner scanner = new Scanner(System.in);
		System.out.print("Firat Name: ");
		firstName = scanner.nextLine();
		System.out.print("Last Name: ");
		lastName = scanner.nextLine();
		System.out.print("Address: ");
		address = scanner.nextLine();
		System.out.print("Email: ");
		email = scanner.nextLine();
		System.out.print("Phone: ");
		phone = scanner.nextLine();
		System.out.print("Notes: ");
		notes = scanner.nextLine();
		
		ContactList oneContact;
		oneContact = new ContactList();
		oneContact.setNewContact(firstName, lastName, address, email, phone, notes);
		System.out.println(oneContact.toString());	
		
	}
}
